    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@playwright/test": "^1.57.0",
    "@types/eslint__js": "^9.14.0",
    "@types/fs-extra": "^11.0.4",
    "@types/mssql": "^9.1.8",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "exceljs": "^4.4.0",
    "husky": "^9.1.7",
    "isomorphic-fetch": "^3.0.0",
    "lint-staged": "^16.2.7",
