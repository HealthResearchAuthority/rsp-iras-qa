    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "hi-base32": "^0.5.1",
    "mssql": "^12.2.0",
    "playwright": "^1.57.0",
    "randexp": "^0.5.3",
    "sharp": "^0.34.5"
  }
