{
  "name": "rsp-iras-qa",
  "version": "1.0.0",
  "description": "Test Automation for the IRAS project, which will provide E2E UI Regression Testing",
  "main": "index.js",
  "scripts": {
    "testTags": "(npx bddgen --tags \"@authSetup or @Smoke\") && (npx playwright test)",
    "testDebug": "(npx bddgen) && (npx playwright test --debug)",
    "test": "(npx bddgen) && (npx playwright test)",
    "test-failed": "(npx bddgen) && (npx playwright test --last-failed)",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "format": "npx prettier . --write",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.48.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "multiple-cucumber-html-reporter": "^3.8.0",
    "playwright-bdd": "^7.5.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.0",
    "dotenv": "^16.4.5",
    "playwright": "^1.48.0"
  }
}
