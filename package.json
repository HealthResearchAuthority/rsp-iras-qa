{
  "name": "rsp-iras-qa",
  "version": "1.0.0",
  "description": "Test Automation for the IRAS project, which will provide E2E UI Regression Testing",
  "main": "index.js",
  "scripts": {
    "testTags": "(npx bddgen --tags \"@authSetup or @Smoke\") && (npx playwright test)",
    "testDebug": "(npx bddgen) && (npx playwright test --debug)",
    "test": "(npx bddgen) && (npx playwright test)",
    "test-failed": "(npx bddgen) && (npx playwright test --last-failed)",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "format": "npx prettier . --write",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
